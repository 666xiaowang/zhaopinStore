<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>lagou</display-name>
  <welcome-file-list>
    <welcome-file>InitServlet</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>EncodingFilter</filter-name>
    <filter-class>com.ronin.filter.EncodingFilter</filter-class>
    <init-param>
      <param-name>charset</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>EncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>LoginFilter</filter-name>
    <filter-class>com.lagou.web.filter.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoginFilter</filter-name>
    <url-pattern>/jianli/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>JianLiFilter</filter-name>
    <filter-class>com.lagou.web.filter.JianLiFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>JianLiFilter</filter-name>
    <url-pattern>/jianli/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>UserServlet</display-name>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>com.lagou.web.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PositionServlet</display-name>
    <servlet-name>PositionServlet</servlet-name>
    <servlet-class>com.lagou.web.servlet.PositionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PositionServlet</servlet-name>
    <url-pattern>/PositionServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>JianLiServlet</display-name>
    <servlet-name>JianLiServlet</servlet-name>
    <servlet-class>com.lagou.web.servlet.JianLiServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JianLiServlet</servlet-name>
    <url-pattern>/JianLiServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CompanyServlet</display-name>
    <servlet-name>CompanyServlet</servlet-name>
    <servlet-class>com.lagou.web.servlet.CompanyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CompanyServlet</servlet-name>
    <url-pattern>/CompanyServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>InitServlet</display-name>
    <servlet-name>InitServlet</servlet-name>
    <servlet-class>com.lagou.web.servlet.InitServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitServlet</servlet-name>
    <url-pattern>/InitServlet</url-pattern>
  </servlet-mapping>
</web-app>